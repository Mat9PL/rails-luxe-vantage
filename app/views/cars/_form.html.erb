<div class="form">
  <%= simple_form_for @car do |f| %>
    <%= f.input :brand %>
    <%= f.input :model %>
    <%= f.input :year %>
    <%= f.input :description %>
    <%= f.input :address %>
    <%= f.input :horse_power %>
    <%= f.input :price %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <%= f.submit class: "form-button", value: "Add car"%>
  <% end %>
</div>
