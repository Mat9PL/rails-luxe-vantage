<%= simple_form_for(@car) do |f| %>
  <%= f.input :brand %>
  <%= f.input :model %>
  <%= f.input :year %>
  <%= f.input :description %>
  <%= f.input :horse_power %>
  <%= f.input :price %>
  <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  <% f.input :user_id, :as => :hidden, :input_text => { :value => 5 } %>
  <%= f.submit %>
<% end %>
